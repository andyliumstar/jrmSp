<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/conference_item5"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/iv_conference_item5_1"
        style="@style/conferenceIv"
        android:layout_marginLeft="200dp" />

    <TextView
        android:id="@+id/tv_conference_item5_1"
        style="@style/conferenceTv"
        android:layout_alignLeft="@id/iv_conference_item5_1"
        android:layout_below="@id/iv_conference_item5_1" />

    <ImageView
        android:id="@+id/iv_conference_item5_2"
        style="@style/conferenceIv"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@id/iv_conference_item5_1" />

    <TextView
        android:id="@+id/tv_conference_item5_2"
        style="@style/conferenceTv"
        android:layout_alignLeft="@id/iv_conference_item5_2"
        android:layout_below="@id/iv_conference_item5_2"/>

    <ImageView
        android:id="@+id/iv_conference_item5_3"
        style="@style/conferenceIv"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@id/iv_conference_item5_2" />

    <TextView
        android:id="@+id/tv_conference_item5_3"
        style="@style/conferenceTv"
        android:layout_alignLeft="@id/iv_conference_item5_3"
        android:layout_below="@id/iv_conference_item5_3" />

    <ImageView
        android:id="@+id/iv_conference_item5_4"
        style="@style/conferenceIv"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@id/iv_conference_item5_3" />

    <TextView
        android:id="@+id/tv_conference_item5_4"
        style="@style/conferenceTv"
        android:layout_alignLeft="@id/iv_conference_item5_4"
        android:layout_below="@id/iv_conference_item5_4" />

    <ImageView
        android:id="@+id/iv_conference_item5_5"
        style="@style/conferenceIv"
        android:layout_marginLeft="30dp"
        android:layout_toRightOf="@id/iv_conference_item5_4" />

    <TextView
        android:id="@+id/tv_conference_item5_5"
        style="@style/conferenceTv"
        android:layout_alignLeft="@id/iv_conference_item5_5"
        android:layout_below="@id/iv_conference_item5_5" />

</RelativeLayout>